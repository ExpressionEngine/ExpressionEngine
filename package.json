{
  "name": "ExpressionEngine",
  "repository": "https://github.com/ExpressionEngine/ExpressionEngine",
  "private": true,
  "scripts": {
    "build:js": "gulp buildJs",
    "watch:js": "gulp watchJs",
    "build:old-js": "babel themes/ee/asset/javascript/src/ -x \".es6\" --out-dir themes/ee/asset/javascript/src/",
    "watch:old-js": "npm run build:old-js -- --watch",
    "build:css": "brunch build --production && cd \"cp-styles\" && (cp build/common.min.css ../themes/ee/cp/css/common.min.css || copy build\\common.min.css ..\\themes\\ee\\cp\\css\\common.min.css) && (cp build/eecms-debug.min.css ../themes/ee/debug/css/eecms-debug.min.css || copy build\\eecms-debug.min.css ..\\themes\\ee\\debug\\css\\eecms-debug.min.css) && (cp -a build/webfonts/. ../themes/ee/webfonts/ || copy build\\webfonts ..\\themes\\ee\\webfonts)",
    "watch:css": "brunch watch --server"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@types/jquery": "^3.5.5",
    "@types/react": "^16.14.6",
    "@types/react-dom": "^16.9.13",
    "auto-reload-brunch": "^2",
    "babel-brunch": "^7",
    "babel-preset-env": "^1.7.0",
    "brunch": "^2.10.17",
    "clean-css-brunch": "^2.10.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^5.0.1",
    "less-brunch": "^2.10.0",
    "sass-brunch": "^2.10.8",
    "typescript": "^3.9.9",
    "typescript-brunch": "^2.3.0",
    "uglify-js-brunch": "^2"
  },
  "dependencies": {
    "node-sass": "^6.0.0"
  }
}
