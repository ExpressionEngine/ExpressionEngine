{
  "name": "expressionengine-pro-themes",
  "repository": "https://github.com/ExpressionEngine/ExpressionEngine",
  "private": true,
  "scripts": {
    "build:js": "gulp buildJs",
    "watch:js": "gulp watchJs",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && mv ./src/js/build/js/index.*.js ../themes/ee/pro/js/fronteditor.min.js",
    "test:unit": "vue-cli-service test:unit",
    "sass": "node-sass './src/scss/fronteditor.scss' '../themes/ee/pro/css/fronteditor.min.css' --output-style compressed",
    "sass-watch": "node-sass --watch './src/scss/fronteditor.scss' '../themes/ee/pro/css/fronteditor.min.css' --output-style compressed",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit"
  },
  "paths": {
    "src": {
      "base": "./src/",
      "img": "./src/img/**/*",
      "fonts": "./src/fonts/**/*",
      "jsFolder": "./src/js/",
      "jsModules": "./src/js/modules/",
      "js": "./src/js/**/*.js",
      "sass": "./src/scss/**/*.scss",
      "sassIncludes": "./src/scss/"
    },
    "dist": {
      "base": "../themes/ee/pro/",
      "css": "../themes/ee/pro/css/",
      "js": "../themes/ee/pro/js/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-eslint": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.19",
    "@vue/cli-plugin-vuex": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/test-utils": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "del": "^3.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-brotli": "^1.2.1",
    "gulp-cached": "^1.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-csscomb": "^3.0.8",
    "gulp-cssnano": "^2.1.3",
    "gulp-filter": "^5.1.0",
    "gulp-gzip": "^1.4.2",
    "gulp-header": "^2.0.9",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^1.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.1.1",
    "gulp-sass-inheritance": "^1.1.0",
    "gulp-twig": "^1.2.0",
    "gulp-uglify": "^3.0.2",
    "gulp-watch": "^5.0.1",
    "merge-stream": "1.0.1",
    "node-sass": "^4.14.1",
    "run-sequence": "^2.2.1",
    "vue-template-compiler": "^2.7.14",
    "yargs": "^12.0.1"
  },
  "dependencies": {
    "core-js": "^3.30.2",
    "sass": "^1.62.1",
    "sass-loader": "^10.4.1",
    "v-tooltip": "^2.1.3",
    "vue": "^2.7.14",
    "vue-onoff-toggle": "^1.1.1",
    "vue-resizable": "^1.3.4",
    "vuex": "^3.4.0"
  }
}
